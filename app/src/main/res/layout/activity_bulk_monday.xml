<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/gymbackg">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Decline Dumbbell Press"
                android:id="@+id/bmw1"
                android:textSize="25dp"
                android:onClick="declinedumbbellpress"
                android:background="#00ffffff"
                android:textColor="#ffffff" />
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button12"
                android:text="Incline Dumbbell Press"
                android:textSize="25dp"
                android:onClick="inclinedumbbellpress"
                android:background="#00ffffff"
                android:textColor="#ffffff" />
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                style="?android:attr/buttonStyleSmall"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Flat Dumbbell Press"
                android:id="@+id/button13"
                android:textSize="25dp"
                android:onClick="flatdumbbellpress"
                android:background="#00ffffff"
                android:textColor="#ffffff" />
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Cable CrossOver Fly"
                android:id="@+id/button14"
                android:textSize="25dp"
                android:onClick="cablecrossoverfly"
                android:background="#00ffffff"
                android:textColor="#ffffff" />
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Push Ups"
                android:id="@+id/button15"
                android:textSize="25dp"
                android:onClick="pushups"
                android:background="#00ffffff"
                android:textColor="#ffffff" />

        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Bar Pushdown"
                android:id="@+id/button16"
                android:textSize="25dp"
                android:onClick="barpushdown"
                android:background="#00ffffff"
                android:textColor="#ffffff" />
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Dumbbell Overhead Extension"
                android:id="@+id/button17"
                android:textSize="25dp"
                android:onClick="overhead"
                android:background="#00ffffff"
                android:textColor="#ffffff" />
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:layout_width="match_parent"
                android:layout_height="62dp"
                android:text="Rope Pushdown"
                android:id="@+id/button18"
                android:textSize="25dp"
                android:onClick="ropepushdown"
                android:background="#00ffffff"
                android:textColor="#ffffff" />
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Tricep Dips"
                android:id="@+id/button19"
                android:textSize="25dp"
                android:onClick="tricepdips"
                android:background="#00ffffff"
                android:textColor="#ffffff" />
        </LinearLayout>


    </LinearLayout>
</ScrollView>
